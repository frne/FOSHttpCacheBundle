<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="fos_httpcache.command.invalidate_path.class">FOS\HttpCacheBundle\Command\InvalidatePathCommand</parameter>
    </parameters>

    <services>
        <service id="fos_httpcache.command.invalidate_path" class="%fos_httpcache.command.invalidate_path.class%">
            <argument type="service" id="fos_http_cache.cache_manager" />
            <tag name="console.command" />
        </service>
    </services>
</container>
